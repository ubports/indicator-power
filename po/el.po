msgid "AC Adapter"
msgstr "Προσαρμογέας εναλλασσόμενου ρεύματος"

#. TRANSLATORS: laptop primary battery
#: ../src/device.c:557 ../src/service.c:442
msgid "Battery"
msgstr "Μπαταρία"

#. TRANSLATORS: battery-backed AC power source
#: ../src/device.c:561
msgid "UPS"
msgstr "Συσκευή αδιάλειπτης παροχής ενέργειας (UPS)"

#. TRANSLATORS: a monitor is a device to measure voltage and current
#: ../src/device.c:565
msgid "Monitor"
msgstr "Συσκευή εποπτείας"

#. TRANSLATORS: wireless mice with internal batteries
#: ../src/device.c:569
msgid "Mouse"
msgstr "Ποντίκι"

#. TRANSLATORS: wireless keyboard with internal battery
#: ../src/device.c:573
msgid "Keyboard"
msgstr "Πληκτρολόγιο"

#. TRANSLATORS: portable device
#: ../src/device.c:577
msgid "PDA"
msgstr "PDA"

#. TRANSLATORS: cell phone (mobile...)
#: ../src/device.c:581
msgid "Cell phone"
msgstr "Κινητό τηλέφωνο"

#. TRANSLATORS: media player, mp3 etc
#: ../src/device.c:585
msgid "Media player"
msgstr "Αναπαραγωγέας πολυμέσων"

#. TRANSLATORS: tablet device
#: ../src/device.c:589
msgid "Tablet"
msgstr "Ταμπλέτα"

#. TRANSLATORS: tablet device
#: ../src/device.c:593
msgid "Computer"
msgstr "Υπολογιστής"

#. TRANSLATORS: unknown device
#: ../src/device.c:597
msgid "Unknown"
msgstr "Άγνωστο"

#: ../src/device.c:637
#, c-format
msgid "estimating…"
msgstr "εκτίμηση..."

#: ../src/device.c:641
#, c-format
msgid "unknown"
msgstr "Άγνωστο"

#. TRANSLATORS: H:MM (hours, minutes) to charge the battery. Example: "1:30 to charge"
#: ../src/device.c:670
#, c-format
msgid "%0d:%02d to charge"
msgstr "Υπολείπονται %0d:%02d για φόρτιση"

#. TRANSLATORS: H:MM (hours, minutes) to discharge the battery. Example: "1:30 left"
#: ../src/device.c:675
#, c-format
msgid "%0d:%02d left"
msgstr "Απομένουν %0d:%02d"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) to charge" the battery.
#. Example: "1 hour 10 minutes to charge"
#: ../src/device.c:710
#, c-format
msgid "%d %s %d %s to charge"
msgstr "Υπολείπονται %d %s ώρα και %d %s λεπτά για φόρτιση"

#: ../src/device.c:711 ../src/device.c:729
msgid "hour"
msgid_plural "hours"
msgstr[0] "ώρα"
msgstr[1] "ώρες"

#: ../src/device.c:712 ../src/device.c:719 ../src/device.c:730
#: ../src/device.c:737
msgid "minute"
msgid_plural "minutes"
msgstr[0] "λεπτό"
msgstr[1] "λεπτά"

#. TRANSLATORS: "Y (minute,minutes) to charge" the battery.
#. Example: "59 minutes to charge"
#: ../src/device.c:718
#, c-format
msgid "%d %s to charge"
msgstr "Υπολείπονται  %d %s λεπτά για φόρτιση"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) left" until the battery's empty.
#. Example: "1 hour 10 minutes left"
#: ../src/device.c:728
#, c-format
msgid "%d %s %d %s left"
msgstr "Απέμειναν %d %s ώρα και %d %s λεπτά"

#. TRANSLATORS: "Y (minute,minutes) left" until the battery's empty.
#. Example: "59 minutes left"
#: ../src/device.c:736
#, c-format
msgid "%d %s left"
msgstr "Απέμειναν %d %s λεπτά"

#. TRANSLATORS: example: "battery (charged)"
#: ../src/device.c:793
#, c-format
msgid "%s (charged)"
msgstr "%s (Φορτισμένη)"

#. TRANSLATORS: example: "battery (time remaining)"
#: ../src/device.c:810
#, c-format
msgid "%s (%s)"
msgstr "%s (%s)"

#. TRANSLATORS: after the icon, a time-remaining string + battery %. Example: "(0:59, 33%)"
#: ../src/device.c:882
#, c-format
msgid "(%s, %.0lf%%)"
msgstr "(%s, %.0lf%%)"

#. TRANSLATORS: after the icon, a time-remaining string Example: "(0:59)"
#: ../src/device.c:887
#, c-format
msgid "(%s)"
msgstr "(%s)"

#. TRANSLATORS: after the icon, a battery %. Example: "(33%)"
#: ../src/device.c:892
#, c-format
msgid "(%.0lf%%)"
msgstr "(%.0lf%%)"

#: ../src/notifier.c:286
msgid "Battery Low"
msgstr "Στάθμη μπαταρίας χαμηλή"

#: ../src/notifier.c:287
msgid "Battery Critical"
msgstr "Στάθμη μπαταρίας κρίσιμα χαμηλή"

#: ../src/notifier.c:289
#, c-format
msgid "%.0f%% charge remaining"
msgstr "%.0f%% φόρτιση απομένει"

#: ../src/notifier.c:321
msgid "OK"
msgstr "Εντάξει"

#: ../src/notifier.c:322
msgid "Battery settings"
msgstr "Ρυθμίσεις μπαταρίας"

#: ../src/service.c:567
msgid "Charge level"
msgstr "Επίπεδο φόρτισης"

#: ../src/service.c:635
msgid "Show Time in Menu Bar"
msgstr "Εμφάνιση της ώρας στη γραμμή μενού"

#: ../src/service.c:639
msgid "Show Percentage in Menu Bar"
msgstr "Εμφάνιση ποσοστού στη μπάρα μενού"

#: ../src/service.c:643
msgid "Power Settings…"
msgstr "Ρυθμίσεις ενέργειας..."

#: ../src/service.c:669
msgid "Adjust brightness automatically"
msgstr "Αυτόματη ρύθμιση φωτεινότητας"

#: ../src/service.c:675
msgid "Battery settings…"
msgstr "Ρυθμίσεις μπαταρίας..."
