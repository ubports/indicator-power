#. TRANSLATORS: system power cord
#: ../src/device.c:524
msgid "AC Adapter"
msgstr "អាដាប់ទ័រ​ចរន្ត​ឆ្លាស់"

#. TRANSLATORS: laptop primary battery
#: ../src/device.c:528 ../src/service.c:443
msgid "Battery"
msgstr "ថ្ម"

#. TRANSLATORS: battery-backed AC power source
#: ../src/device.c:532
msgid "UPS"
msgstr "យូភីអេស"

#. TRANSLATORS: a monitor is a device to measure voltage and current
#: ../src/device.c:536
msgid "Monitor"
msgstr "ម៉ូនីទ័រ"

#. TRANSLATORS: wireless mice with internal batteries
#: ../src/device.c:540
msgid "Mouse"
msgstr "កណ្ដុរ"

#. TRANSLATORS: wireless keyboard with internal battery
#: ../src/device.c:544
msgid "Keyboard"
msgstr "ក្ដារចុច"

#. TRANSLATORS: portable device
#: ../src/device.c:548
msgid "PDA"
msgstr "PDA"

#. TRANSLATORS: cell phone (mobile...)
#: ../src/device.c:552
msgid "Cell phone"
msgstr "ទូរស័ព្ទចល័ត"

#. TRANSLATORS: media player, mp3 etc
#: ../src/device.c:556
msgid "Media player"
msgstr "កម្មវិធី​ចាក់​មេឌៀ"

#. TRANSLATORS: tablet device
#: ../src/device.c:560
msgid "Tablet"
msgstr "ថេប្លេត"

#. TRANSLATORS: tablet device
#: ../src/device.c:564
msgid "Computer"
msgstr "កុំព្យូទ័រ"

#. TRANSLATORS: unknown device
#: ../src/device.c:568
msgid "Unknown"
msgstr "មិនស្គាល់"

#: ../src/device.c:608
#, c-format
msgid "estimating…"
msgstr "ការ​ប៉ាន់ស្មាន…"

#: ../src/device.c:612
#, c-format
msgid "unknown"
msgstr "មិន​ស្គាល់"

#. TRANSLATORS: H:MM (hours, minutes) to charge the battery. Example: "1:30 to charge"
#: ../src/device.c:641
#, c-format
msgid "%0d:%02d to charge"
msgstr "%0d:%02d ចំពោះ​កម្រិត​ថ្ម"

#. TRANSLATORS: H:MM (hours, minutes) to discharge the battery. Example: "1:30 left"
#: ../src/device.c:646
#, c-format
msgid "%0d:%02d left"
msgstr "%0d:%02d នៅសល់"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) to charge" the battery.
#. Example: "1 hour 10 minutes to charge"
#: ../src/device.c:681
#, c-format
msgid "%d %s %d %s to charge"
msgstr "%d %s %d %s ចំពោះ​កម្រិត​ថ្ម"

#: ../src/device.c:682 ../src/device.c:700
msgid "hour"
msgid_plural "hours"
msgstr[0] "ម៉ោង"

#: ../src/device.c:683 ../src/device.c:690 ../src/device.c:701
#: ../src/device.c:708
msgid "minute"
msgid_plural "minutes"
msgstr[0] "នាទី"

#. TRANSLATORS: "Y (minute,minutes) to charge" the battery.
#. Example: "59 minutes to charge"
#: ../src/device.c:689
#, c-format
msgid "%d %s to charge"
msgstr "%d %s ចំពោះ​កម្រិត​ថ្ម"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) left" until the battery's empty.
#. Example: "1 hour 10 minutes left"
#: ../src/device.c:699
#, c-format
msgid "%d %s %d %s left"
msgstr "%d %s %d %s នៅ​សល់"

#. TRANSLATORS: "Y (minute,minutes) left" until the battery's empty.
#. Example: "59 minutes left"
#: ../src/device.c:707
#, c-format
msgid "%d %s left"
msgstr "%d %s នៅ​សល់"

#. TRANSLATORS: example: "battery (charged)"
#: ../src/device.c:764
#, c-format
msgid "%s (charged)"
msgstr "%s (បាន​បញ្ចូល​)"

#. TRANSLATORS: example: "battery (time remaining)"
#: ../src/device.c:781
#, c-format
msgid "%s (%s)"
msgstr "%s (%s)"

#. TRANSLATORS: after the icon, a time-remaining string + battery %. Example: "(0:59, 33%)"
#: ../src/device.c:853
#, c-format
msgid "(%s, %.0lf%%)"
msgstr "(%s, %.0lf%%)"

#. TRANSLATORS: after the icon, a time-remaining string Example: "(0:59)"
#: ../src/device.c:858
#, c-format
msgid "(%s)"
msgstr "(%s)"

#. TRANSLATORS: after the icon, a battery %. Example: "(33%)"
#: ../src/device.c:863
#, c-format
msgid "(%.0lf%%)"
msgstr "(%.0lf%%)"

#: ../src/notifier.c:286
msgid "Battery Low"
msgstr "ថ្មទាប"

#: ../src/notifier.c:287
msgid "Battery Critical"
msgstr "ថ្មទាបណាស់"

#: ../src/notifier.c:289
#, c-format
msgid "%.0f%% charge remaining"
msgstr "ការចោទប្រកាន់នៅសល់ %.0f%%"

#: ../src/notifier.c:321
msgid "OK"
msgstr "យល់ព្រម"

#: ../src/notifier.c:322
msgid "Battery settings"
msgstr "ការកំណត់ថ្ម"

#: ../src/service.c:568
msgid "Charge level"
msgstr "កម្រិត​បញ្ចូល​ថ្ម"

#: ../src/service.c:636
msgid "Show Time in Menu Bar"
msgstr "បង្ហាញ​ពេលវេលា​ក្នុង​របារ​ម៉ឺនុយ"

#: ../src/service.c:640
msgid "Show Percentage in Menu Bar"
msgstr "បង្ហាញ​ភាគ​រយ​ក្នុង​របារ​ម៉ឺនុយ"

#: ../src/service.c:644
msgid "Power Settings…"
msgstr "ការ​កំណត់​​ថាមពល…"

#: ../src/service.c:670
msgid "Adjust brightness automatically"
msgstr "លៃតម្រូវកម្រិតពន្លឺដោយស្វ័យប្រវត្តិ"

#: ../src/service.c:678
msgid "Flashlight"
msgstr "ពិល"

#: ../src/service.c:684
msgid "Warning: Heavy use can damage the LED!"
msgstr "ការព្រមាន៖ ការប្រើប្រាស់ខ្លាំងអាចធ្វើឱ្យ LED ខូច!"

#: ../src/service.c:690
msgid "Battery settings…"
msgstr "ការ​កំណត់​ថ្ម…"
