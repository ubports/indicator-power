#: ../src/device.c:689
#, c-format
msgid "%d %s to charge"
msgstr "%d %s للشحن"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) left" until the battery's empty.
#. Example: "1 hour 10 minutes left"
#: ../src/device.c:699
#, c-format
msgid "%d %s %d %s left"
msgstr "%d %s %d %s متبقي"

