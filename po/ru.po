#. TRANSLATORS: H:MM (hours, minutes) to charge the battery. Example: "1:30 to charge"
#: ../src/device.c:641
#, c-format
msgid "%0d:%02d to charge"
msgstr "%0d:%02d до полного заряда батареи"

#. TRANSLATORS: H:MM (hours, minutes) to discharge the battery. Example: "1:30 left"
#: ../src/device.c:646
#, c-format
msgid "%0d:%02d left"
msgstr "Осталось %0d:%02d"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) to charge" the battery.
#. Example: "1 hour 10 minutes to charge"
#: ../src/device.c:681
#, c-format
msgid "%d %s %d %s to charge"
msgstr "%d %s %d %s до полного заряда батареи"

#: ../src/device.c:682 ../src/device.c:700
msgid "hour"
msgid_plural "hours"
msgstr[0] "час"
msgstr[1] "часа"
msgstr[2] "часов"

#: ../src/device.c:683 ../src/device.c:690 ../src/device.c:701
#: ../src/device.c:708
msgid "minute"
msgid_plural "minutes"
msgstr[0] "минута"
msgstr[1] "минуты"
msgstr[2] "минут"

#. TRANSLATORS: "Y (minute,minutes) to charge" the battery.
#. Example: "59 minutes to charge"
#: ../src/device.c:689
#, c-format
msgid "%d %s to charge"
msgstr "%d %s до полного заряда батареи"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) left" until the battery's empty.
#. Example: "1 hour 10 minutes left"
#: ../src/device.c:699
#, c-format
msgid "%d %s %d %s left"
msgstr "Осталось %d %s %d %s"

#. TRANSLATORS: "Y (minute,minutes) left" until the battery's empty.
#. Example: "59 minutes left"
#: ../src/device.c:707
#, c-format
msgid "%d %s left"
msgstr "Осталось %d %s"

#. TRANSLATORS: example: "battery (charged)"
#: ../src/device.c:764
#, c-format
msgid "%s (charged)"
msgstr "%s (заряжено)"

#. TRANSLATORS: example: "battery (time remaining)"
#: ../src/device.c:781
#, c-format
msgid "%s (%s)"
msgstr "%s (%s)"

#. TRANSLATORS: after the icon, a time-remaining string + battery %. Example: "(0:59, 33%)"
#: ../src/device.c:853
#, c-format
msgid "(%s, %.0lf%%)"
msgstr "(%s, %.0lf%%)"

#. TRANSLATORS: after the icon, a time-remaining string Example: "(0:59)"
#: ../src/device.c:858
#, c-format
msgid "(%s)"
msgstr "(%s)"

#. TRANSLATORS: after the icon, a battery %. Example: "(33%)"
#: ../src/device.c:863
#, c-format
msgid "(%.0lf%%)"
msgstr "(%.0lf%%)"

#: ../src/notifier.c:286
msgid "Battery Low"
msgstr "Низкий заряд батареи"

#: ../src/notifier.c:287
msgid "Battery Critical"
msgstr "Критически низкий заряд батареи"

#: ../src/notifier.c:289
#, c-format
msgid "%.0f%% charge remaining"
msgstr "Осталось %.0f%% заряда"

#: ../src/notifier.c:321
msgid "OK"
msgstr "ОК"

#: ../src/notifier.c:322
msgid "Battery settings"
msgstr "Параметры батареи"

#: ../src/service.c:568
msgid "Charge level"
msgstr "Уровень заряда"

#: ../src/service.c:636
msgid "Show Time in Menu Bar"
msgstr "Показывать время на панели меню"

#: ../src/service.c:640
msgid "Show Percentage in Menu Bar"
msgstr "Показывать проценты на панели меню"

#: ../src/service.c:644
msgid "Power Settings…"
msgstr "Параметры питания…"

#: ../src/service.c:670
msgid "Adjust brightness automatically"
msgstr "Автояркость"

#: ../src/service.c:678
msgid "Flashlight"
msgstr "Фонарик"

#: ../src/service.c:684
msgid "Warning: Heavy use can damage the LED!"
msgstr "Предупреждение: Интенсивное использование может повредить светодиод!"

#: ../src/service.c:690
msgid "Battery settings…"
msgstr "Параметры батареи…"
