#: ../src/device.c:882
#, c-format
msgid "(%s, %.0lf%%)"
msgstr "(%s, %.0lf%%)"

#. TRANSLATORS: after the icon, a time-remaining string Example: "(0:59)"
#: ../src/device.c:887
#, c-format
msgid "(%s)"
msgstr "(%s)"

