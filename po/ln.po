#: ../src/notifier.c:287
msgid "Battery Critical"
msgstr "Batéri esili moto"

#: ../src/notifier.c:289
#, c-format
msgid "%.0f%% charge remaining"
msgstr "Etikali mwa %.0f%%"

#: ../src/notifier.c:321
msgid "OK"
msgstr "Iyo"

#: ../src/notifier.c:322
msgid "Battery settings"
msgstr "Palamɛ́tɛlɛ ya batéri"

#: ../src/service.c:567
msgid "Charge level"
msgstr "Motango ya sarze"

#: ../src/service.c:635
msgid "Show Time in Menu Bar"
msgstr "Monisa ngonga na barɛ ya meni"

#: ../src/service.c:639
msgid "Show Percentage in Menu Bar"
msgstr "Monisa ya monkámá na batéri na barɛ ya meni"

#: ../src/service.c:643
msgid "Power Settings…"
