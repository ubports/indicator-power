msgid "unknown"
msgstr "disconnotu"

#. TRANSLATORS: H:MM (hours, minutes) to charge the battery. Example: "1:30 to charge"
#: ../src/device.c:670
#, c-format
msgid "%0d:%02d to charge"
msgstr "%0d:%02d pro carrigare"

#. TRANSLATORS: H:MM (hours, minutes) to discharge the battery. Example: "1:30 left"
#: ../src/device.c:675
#, c-format
msgid "%0d:%02d left"
msgstr "%0d:%02d mancante"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) to charge" the battery.
#. Example: "1 hour 10 minutes to charge"
#: ../src/device.c:710
#, c-format
msgid "%d %s %d %s to charge"
msgstr "%d %s %d %s a sa càrriga cumpleta"

#: ../src/device.c:711 ../src/device.c:729
msgid "hour"
msgid_plural "hours"
msgstr[0] "ora"
msgstr[1] "oras"

#: ../src/device.c:712 ../src/device.c:719 ../src/device.c:730
#: ../src/device.c:737
msgid "minute"
msgid_plural "minutes"
msgstr[0] "minutu"
msgstr[1] "minutos"

#. TRANSLATORS: "Y (minute,minutes) to charge" the battery.
#. Example: "59 minutes to charge"
#: ../src/device.c:718
#, c-format
msgid "%d %s to charge"
msgstr "%d %s pro carrigare"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) left" until the battery's empty.
#. Example: "1 hour 10 minutes left"
#: ../src/device.c:728
#, c-format
msgid "%d %s %d %s left"
msgstr "%d %s %d %s mancante"

#. TRANSLATORS: "Y (minute,minutes) left" until the battery's empty.
#. Example: "59 minutes left"
#: ../src/device.c:736
#, c-format
msgid "%d %s left"
msgstr "%d %s mancante"

#. TRANSLATORS: example: "battery (charged)"
#: ../src/device.c:793
#, c-format
msgid "%s (charged)"
msgstr "%s (carrigada)"

#. TRANSLATORS: example: "battery (time remaining)"
#: ../src/device.c:810
#, c-format
msgid "%s (%s)"
msgstr "%s (%s)"

#. TRANSLATORS: after the icon, a time-remaining string + battery %. Example: "(0:59, 33%)"
#: ../src/device.c:882
#, c-format
msgid "(%s, %.0lf%%)"
msgstr "(%s, %.0lf%%)"

#. TRANSLATORS: after the icon, a time-remaining string Example: "(0:59)"
#: ../src/device.c:887
#, c-format
msgid "(%s)"
msgstr "(%s)"

#. TRANSLATORS: after the icon, a battery %. Example: "(33%)"
#: ../src/device.c:892
#, c-format
msgid "(%.0lf%%)"
msgstr "(%.0lf%%)"

#: ../src/notifier.c:286
msgid "Battery Low"
msgstr "Bateria bàscia"

#: ../src/notifier.c:287
msgid "Battery Critical"
msgstr "Bateria crìtica"

#: ../src/notifier.c:289
#, c-format
msgid "%.0f%% charge remaining"
msgstr "%.0f%% càrriga mancante"

#: ../src/notifier.c:321
msgid "OK"
msgstr "AB"

#: ../src/notifier.c:322
msgid "Battery settings"
msgstr "Cunfiguratziones de sa bateria"

#: ../src/service.c:567
msgid "Charge level"
msgstr "Livellu de càrriga"

#: ../src/service.c:635
msgid "Show Time in Menu Bar"
msgstr "Ammustra su tempus in sa barra de menù"

#: ../src/service.c:639
msgid "Show Percentage in Menu Bar"
msgstr "Ammustra pertzentuale in sa barra de menù"

#: ../src/service.c:643
msgid "Power Settings…"
msgstr "Cunfiguratziones de s'energia…"

#: ../src/service.c:669
msgid "Adjust brightness automatically"
msgstr "Acontza su lugore in automàticu"

#: ../src/service.c:675
msgid "Battery settings…"
msgstr "Cunfiguratziones de sa bateria…"
