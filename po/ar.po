#: ../src/device.c:646
#, c-format
msgid "%0d:%02d left"
msgstr "%0d:%02d متبقي"

#. TRANSLATORS: "X (hour,hours) Y (minute,minutes) to charge" the battery.
#. Example: "1 hour 10 minutes to charge"
#: ../src/device.c:681
#, c-format
msgid "%d %s %d %s to charge"
msgstr "%d %s %d %s للشحن"

#: ../src/device.c:682 ../src/device.c:700
msgid "hour"
msgid_plural "hours"
msgstr[0] "أقل من ساعة"
msgstr[1] "ساعة واحدة"
msgstr[2] "ساعتين"
msgstr[3] "ساعات"
msgstr[4] "ساعة"
